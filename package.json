{
  "name": "date-holidays-parser",
  "version": "1.4.0-0",
  "description": "parser for worldwide holidays",
  "keywords": [
    "holidays",
    "parser",
    "world"
  ],
  "homepage": "https://github.com/commenthol/date-holidays-parser",
  "bugs": {
    "url": "https://github.com/commenthol/date-holidays-parser/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/date-holidays-parser.git"
  },
  "license": "ISC",
  "author": "commenthol <commenthol@gmail.com>",
  "contributors": [],
  "main": "lib",
  "directories": {
    "lib": "lib",
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "all": "npm-run-all clean lint build build:es5 test",
    "build": "babel -d lib src",
    "build:es5": "BABEL_ENV=es5 babel -d es src",
    "clean": "rimraf lib es coverage .nyc_output",
    "clean:all": "npm-run-all clean:node_modules clean",
    "clean:node_modules": "rimraf node_modules",
    "coverage": "nyc -r text -r html npm test",
    "doc": "jsdox -o docs src/Holidays.js",
    "lint": "eslint '**/*.js'",
    "prepublishOnly": "npm run all",
    "test": "mocha",
    "test:tz": "TZ=UTC-10 npm test && TZ=UTC+10 npm test",
    "zuul": "zuul test/Holidays.test.js"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "env"
        ]
      },
      "es5": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current",
                "browsers": [
                  "last 2 versions",
                  "ie >= 11"
                ]
              },
              "modules": false,
              "loose": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ]
  },
  "dependencies": {
    "astronomia": "^2.0.1",
    "caldate": "^1.0.3-0",
    "date-bengali-revised": "^1.0.1-0",
    "date-chinese": "^1.0.6-0",
    "date-easter": "^0.2.4-0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.1",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2",
    "moment-timezone": "^0.5.23"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "core-js": "^2.5.5",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "hashtree": "^0.7.0",
    "mocha": "^5.1.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "serialize-to-js": "^1.2.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "maintainers": [
    "commenthol <commenthol@gmail.com>"
  ]
}
